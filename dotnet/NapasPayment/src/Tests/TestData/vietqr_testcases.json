        "amount": 0,
        "bankBIN": "970415",
        "accountNumber": "1234567890",
        "note": "",
        "currency": "VND",
        "countryCode": "VN"
      },
      "expected": {
        "expectedQRCode": "00020101021238540010A00000072701240006970415011012345678900208QRIBFTTA5303704540105802VN63049F5F"
      },
      "shouldThrowException": false
    },
