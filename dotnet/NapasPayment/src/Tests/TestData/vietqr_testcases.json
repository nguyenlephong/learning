{
  "version": "1.0",
  "description": "VietQR GenerateWithParams Function Test Cases",
  "testCases": [
    {
      "testName": "Basic_Valid_QR_With_Amount_And_Note",
      "description": "Test basic QR generation with amount and note",
      "input": {
        "oneTime": true,
        "serviceType": "QRIBFTTA",
        "amount": 100000,
        "bankBIN": "970423",
        "accountNumber": "0011009950446",
        "note": "Test payment",
        "currency": "VND",
        "countryCode": "VN"
      },
      "expected": {
        "expectedQRCode": "00020101021238570010A00000072701270006970423011300110099504460208QRIBFTTA530370454061000005802VN62160812Test payment630427AA"
      },
      "shouldThrowException": false
    },
    {
      "testName": "Valid_QR_No_Amount_No_Note",
      "description": "Test QR generation without amount and note",
      "input": {
        "oneTime": true,
